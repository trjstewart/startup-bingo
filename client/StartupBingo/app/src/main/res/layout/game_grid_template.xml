<?xml version="1.0" encoding="utf-8"?>
<co.startupbingo.startupbingo.ui.SquareGameTileLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/game_tile_base_layout">
    <RelativeLayout
        android:id="@+id/game_tile_click_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            tools:text="Startup"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:ellipsize="end"
            android:id="@+id/game_tile_text_view" />
    </RelativeLayout>
</co.startupbingo.startupbingo.ui.SquareGameTileLayout>